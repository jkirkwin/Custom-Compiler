void main() {
    string[5] strings;
    strings[0] = "ABC...Z";
    strings[1] = "!,.:_{}";
    strings[2] = "0123456789"
    strings[3] = "abc...z"
    strings[4] = ""; // I'm not 100% sure whether we'll support the empty string.

    printStrings(strings);
}

void printStrings(string[5] strings) {
    int i;
    i = 5;
    while (0 < i) {
        print(strings[i]);

        if (i != 1) {
            println ", ";
        }

        i = i - 1;
    }
}
