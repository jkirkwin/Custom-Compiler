void main() {
    string[5] strings;
    strings[0] = "ABC...Z";
    strings[1] = "!,.:_{}";
    strings[2] = "0123456789";
    strings[3] = "abc...z";
    strings[4] = " ";

    printStrings(strings);
}

void printStrings(string[5] strings) {
    int i;
    i = 5;
    while (0 < i) {
        print(strings[i]);

        if (1 < i) {
            println ", ";
        }

        i = i - 1;
    }
}
